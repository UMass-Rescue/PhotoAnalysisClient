{
  "name": "electron-opskit",
  "version": "0.1.0",
  "private": true,
  "main": "./main/index.js",
  "homepage": ".",
  "author": "Jagath Jai Kumar",
  "license": "MIT",
  "devDependencies": {
    "axios-mock-adapter": "^1.18.2",
    "child_process": "^1.0.2",
    "concurrently": "^5.2.0",
    "electron": "^9.1.2",
    "electron-builder": "^22.8.0",
    "run-script-os": "^1.1.1",
    "typescript": "^3.9.7",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.1",
    "chart.js": "^2.8.0",
    "clsx": "^1.0.4",
    "history": "^4.9.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.6",
    "react-perfect-scrollbar": "^1.5.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "recompose": "^0.30.0",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2",
    "validate.js": "^0.13.1",
    "ps-tree": "^1.2.0"
  },
  "scripts": {
    "start-flask": "run-script-os",
    "start-flask:default": "export BACKEND_PORT=8000 && python3 backend/main.py",
    "start-flask:windows": "set BACKEND_PORT=8000 && python3 backend/main.py",
    "start-electron": "run-script-os",
    "start-electron:default": "export DISPLAY=:0.0 && export ELECTRON_START_URL=http://localhost:3000 && electron main",
    "start-electron:windows": "set DISPLAY=:0.0 && set ELECTRON_START_URL=http://localhost:3000 && electron main",
    "start-react": "run-script-os",
    "start-react:default": "export BROWSER=none && react-scripts start",
    "start-react:windows": "set BROWSER=none && react-scripts start",
    "debug": "run-script-os",
    "debug:default": "export ENVIRONMENT=debug && concurrently 'npm run start-react' 'npm run start-flask' 'sleep 5 && npm run start-electron' ",
    "debug:windows": "set ENVIRONMENT=debug && concurrently 'npm run start-react' 'npm run start-flask' 'sleep 5 && npm run start-electron'",
    "dev": "run-script-os",
    "dev:default": "export ENVIRONMENT=development && concurrently 'npm run start-react' 'sleep 5 && npm run start-electron' ",
    "dev:windows": "set ENVIRONMENT=development && concurrently 'npm run start-react' 'sleep 5 && npm run start-electron'",
    "prod": "run-script-os",
    "prod:default": "export ENVIRONMENT=production && npm run start-electron",
    "prod:windows": "set ENVIRONMENT=production && npm run start-electron",
    "setup": "npm install && pip3 install -r backend/requirements-dev.txt",
    "build": "react-scripts build",
    "build-python": "run-script-os",
    "build-python:default": "virtualenv -p python3 python/venv-app && ./python/venv-app/bin/pip install -r backend/requirements.txt && ./python/venv-app/bin/pyinstaller -F ./python/main.py --workpath ./backend_build --distpath ./backend_dist --specpath ./backend_spec",
    "build-python:windows": "virtualenv -p python3 python\\venv-app && ./python\\venv-app\\bin\\pip install -r backend\\requirements.txt && ./python\\venv-app\\bin\\pyinstaller -F ./python/main.py --workpath ./backend_build --distpath ./backend_dist --specpath ./backend_spec",
    "clean": "run-script-os",
    "clean:default": "rm -rf python/venv-app backend_build backend_dist backend_spec build dist",
    "clean:windows": "rmdir python\\venv-app backend_build backend_dist backend_spec build dist",
    "test": "react-scripts test",
    "test-python": "pytest",
    "eject": "react-scripts eject",
    "package-electron": "npm run clean && npm run build && npm run build-python && electron-builder build -c.extraMetadata.main=main/index.js --publish never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "nonExistentID",
    "files": [
      "build/**/*",
      "main/**/*",
      "node_modules/**/*",
      "public/**/*",
      "backend_dist/**/*"
    ]
  }
}
